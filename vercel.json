{
  "version": 2,
  "buildCommand": "npm run build:frontend",
  "outputDirectory": "frontend/build",
  "installCommand": "npm install",
  "env": {
    "CI": "",
    "NODE_ENV": "production",
    "SKIP_PREFLIGHT_CHECK": "true",
    "NODE_OPTIONS": "--openssl-legacy-provider"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}